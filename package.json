  "main": "index.js",
  "scripts": {
    "test": "node_modules/.bin/mocha -R spec",
    "posttest": "node_modules/.bin/mocha -r blanket -R travis-cov && ( node_modules/.bin/mocha -r blanket -R travis-cov >| coverage.html )"
